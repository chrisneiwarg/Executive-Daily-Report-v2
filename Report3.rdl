  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DealID">
            <Value>=Parameters!DealID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 
ISNULL(l.id_websit_assigned,'') as Item#, 
l.name, 
floor(l.quantity) as Quantity, 
FORMAT(l.price, 'C') as SalesPrice, 
FORMAT(ISNULL(l.base_cost,0),'C') as BaseCost, 
FORMAT(l.hs_margin, 'C') as Margin,
ISNULL(l.vendor, 'Error') as Vendor,
dla.dealid as DealID


from dbo.LineItem l

Left Join dbo.deallineitemassociations dla on l.ObjectId = dla.objectID

where dla.dealid IS NOT NULL AND @DealID = dla.DealId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
