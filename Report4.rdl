      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>3cf21179-e34e-459c-85eb-18379961c391</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@DealID">
            <Value>=Parameters!DealID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 

p.objectID, 
IIF(p.status='Rejected/Returned','Pending Sales Clarification',
	IIF(p.status='Unable to Source','Sourcing',
		p.status)) as Status,
p.product, 
pd.dealid

from dbo.productsourcing p

Left Join dbo.productsourcingdealassociations pd on p.objectID = pd.ProductSourcingID


where status not in ('Sourcing Complete','Request Closed Per Deal','Cross Referenced') AND pd.dealid IS NOT NULL AND @DealID = pd.DealId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="objectID">
          <DataField>objectID</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="product">
          <DataField>product</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dealid">
          <DataField>dealid</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
