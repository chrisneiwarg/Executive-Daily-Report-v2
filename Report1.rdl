  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Engagements_SubReport</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@OwnerID">
            <Value>=Parameters!OwnerID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select 

floor(e.EngagementId) as EngagementID,
e.hubspot_owner_id,
CONCAT(o.FirstName,' ', o.LastName) as Name,
e.Type, 

CONCAT(ISNULL(e.Subject,''),ISNULL(e.Body,''),ISNULL(e.CallOutcome,''),ISNULL(e.MeetingOutcome,'')) as Details,

--ISNULL(e.Body,'') as Body,
--ISNULL(e.CallOutcome,'') as CallOutcome,
--ISNULL(e.MeetingOutcome,'') as MeetingOutcome,
--ISNULL(e.Subject,'') as Subject,
--ISNULL(eda.DealId,'') as AssocDeal,
d.dealname as DealName

from dbo.Engagement e

Left Join dbo.Owner o on e.hubspot_owner_id = o.hubspot_owner_id
Left Join dbo.engagementdealassociations eda on e.EngagementId = eda.engagementid
Left Join dbo.EngagementContactAssociations eca on e.EngagementId = eca.EngagementId
Left Join dbo.EngagementCompanyAssociations ecca on e.EngagementId = ecca.EngagementId
Left Join dbo.Deal d on eda.DealID = d.DealId


where e.CreatedAt &gt;DATEADD(DAY, DATEDIFF(DAY, '19000101', GETDATE()), '19000101')  AND e.hubspot_owner_id = @OwnerID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
